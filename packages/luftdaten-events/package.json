{
  "name": "@kuuki/luftdaten-events",
  "version": "1.0.4",
  "description": "An enhanced version of @kuuki/luftdaten with events support",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "prebuild": "rimraf dist",
    "build": "./node_modules/.bin/tsc",
    "dev": "./node_modules/.bin/tsnd --ignore-watch node_modules --respawn --clear --no-notify ./src --watch",
    "docs": "rimraf ./../../docs/luftdaten-events && typedoc --out ./../../docs/luftdaten-events src"
  },
  "keywords": [
    "luftdaten",
    "api",
    "javascript",
    "node",
    "js"
  ],
  "author": "Dacturne",
  "license": "ISC",
  "dependencies": {
    "@kuuki/luftdaten": "^1.1.2",
    "level-ttl": "^3.1.1",
    "leveldown": "^5.4.1",
    "levelup": "^4.3.2",
    "node-cron": "^2.0.3",
    "node-fetch": "^2.6.0"
  },
  "devDependencies": {
    "@types/level-ttl": "^3.1.0",
    "@types/node": "^12.7.11",
    "@types/node-fetch": "^2.5.4",
    "rimraf": "^3.0.0",
    "ts-node": "^8.5.4",
    "ts-node-dev": "^1.0.0-pre.43",
    "tsc": "^1.20150623.0",
    "tslint": "^5.20.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-lines-between-class-members": "^1.3.6",
    "typedoc": "^0.15.3",
    "typescript": "^3.8.0-dev.20191224"
  },
  "publishConfig": {
    "access": "public"
  }
}
